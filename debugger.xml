<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="showValuesInline" value="false" />
      <sort-values>true</sort-values>
      <show-values-inline>false</show-values-inline>
    </data-views>
    <general />
    <debuggers>
      <debugger id="javascript">
        <configuration captureAsyncStackTraces="false">
          <custom-object-presentation>
            <properties />
          </custom-object-presentation>
          <stepping-filters-enabled>false</stepping-filters-enabled>
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>